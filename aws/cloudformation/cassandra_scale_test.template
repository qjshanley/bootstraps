{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Resources": {
    "SubnetDataUSEast1AToNATRouting": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": {
          "Ref": "SubnetData01USEast1A"
        },
        "RouteTableId": {
          "Ref": "NATRouting"
        }
      }
    },
    "SubnetDataUSEast1BToNATRouting": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": {
          "Ref": "SubnetData01USEast1B"
        },
        "RouteTableId": {
          "Ref": "NATRouting"
        }
      }
    },
    "SubnetDataUSEast1DToNATRouting": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": {
          "Ref": "SubnetData01USEast1D"
        },
        "RouteTableId": {
          "Ref": "NATRouting"
        }
      }
    },
    "CassandraScaleTestVPC": {
      "Type": "AWS::EC2::VPC",
      "Properties": {
        "CidrBlock": "10.0.0.0/16",
        "EnableDnsSupport": true,
        "EnableDnsHostnames": true,
        "Tags": [
          {
            "Key": "Name",
            "Value": "Cassandra Scale Test"
          }
        ]
      }
    },
    "SubnetDMZ01": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "AvailabilityZone": "us-east-1a",
        "CidrBlock": "10.0.0.0/24",
        "VpcId": {
          "Ref": "CassandraScaleTestVPC"
        }
      }
    },
    "SubnetData01USEast1A": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "AvailabilityZone": "us-east-1a",
        "CidrBlock": "10.0.1.0/24",
        "VpcId": {
          "Ref": "CassandraScaleTestVPC"
        }
      }
    },
    "SubnetData01USEast1B": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "AvailabilityZone": "us-east-1b",
        "CidrBlock": "10.0.2.0/24",
        "VpcId": {
          "Ref": "CassandraScaleTestVPC"
        }
      }
    },
    "SubnetData01USEast1D": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "AvailabilityZone": "us-east-1d",
        "CidrBlock": "10.0.3.0/24",
        "VpcId": {
          "Ref": "CassandraScaleTestVPC"
        }
      }
    },
    "IGW": {
      "Type": "AWS::EC2::InternetGateway",
      "Properties": {}
    },
    "IGWToCassandraScaleTestVPC": {
      "Type": "AWS::EC2::VPCGatewayAttachment",
      "Properties": {
        "InternetGatewayId": {
          "Ref": "IGW"
        },
        "VpcId": {
          "Ref": "CassandraScaleTestVPC"
        }
      }
    },
    "NATElasticIP": {
      "Type": "AWS::EC2::EIP",
      "Properties": {
        "Domain": "vpc"
      }
    },
    "NATRouting": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "VpcId": {
          "Ref": "CassandraScaleTestVPC"
        }
      }
    },
    "RouteToNAT": {
      "Type": "AWS::EC2::Route",
      "Properties": {
        "RouteTableId": {
          "Ref": "NATRouting"
        },
        "DestinationCidrBlock": "0.0.0.0/0",
        "NatGatewayId": {
          "Ref": "NAT"
        }
      },
      "DependsOn": [
        "IGW"
      ]
    },
    "NAT": {
      "Type": "AWS::EC2::NatGateway",
      "Properties": {
        "AllocationId": {
          "Fn::GetAtt": [
            "NATElasticIP",
            "AllocationId"
          ]
        },
        "SubnetId": {
          "Ref": "SubnetDMZ01"
        }
      }
    }
  }
}
