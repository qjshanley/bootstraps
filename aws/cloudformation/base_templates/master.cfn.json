{
	"AWSTemplateFormatVersion": "2010-09-09",
	"Description": "Do a bunch of things and stuff.",
	"Parameters": {
		"GitBranch": {
			"Description": "The Git branch to use.",
			"Type": "String",
			"Default": "master"
		},
		"VPCTemplate": {
			"Description": "CloudFormation template to create the VPC scaffolding.",
			"Type": "String",
			"Default": "vpc.cfn.json"
		}
	},
	"Resources": {
		"VPCScaffold": {
			"Type": "AWS::CloudFormation::Stack",
			"Properties": {
				"TemplateURL": { "Fn::Join" : ["/", [ "https://s3.amazonaws.com/qub3r-cfn-templates/toy_app", { "Ref" : "VPCTemplate" }]]},
				"Parameters": {
				}
			}
		}
	}
}
